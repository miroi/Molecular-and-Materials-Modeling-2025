#!/bin/bash

# change accordingly to your MOPAC installation
export MOPAC_SRC=/home/milias/work/software/mopac/mopac-23.1.2-linux

export PATH=$MOPAC_SRC/bin:$PATH
export LD_LIBRARY_PATH=$MOPAC_SRC/lib:$LD_LIBRARY_PATH

echo -e "\n Running MOPAC 23.1.2 on server \"`hostname`\"  "

which mopac; ldd `which mopac`
echo

mopac CO2_pm7.mop

echo -e "\n MOPAC run ends on \c"; date

exit 0
